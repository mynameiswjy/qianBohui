/**app.wxss**/
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
} 
.wraps {
  font-family:PingFangSC-Regular;
}
button::after{
  border: 0;
  width: inherit;
  height: inherit;
}
button {
  border: 0;
  background: none;
  padding: 0;
  text-align:inherit;
  line-height:inherit;
  color: inherit;
}
input{
  outline:none;
  border:none;
  list-style: none;
}
.flex {
  display: flex;
}
@font-face {
  font-family:"iconfont";
  src: url('//at.alicdn.com/t/font_810881_vdsmyx7br89.eot?t=1565244928517');
  /* IE9 */
  src: url('//at.alicdn.com/t/font_810881_vdsmyx7br89.eot?t=1565244928517#iefix') format('embedded-opentype'),
    /* IE6-IE8 */
  url('data:application/x-font-woff2;charset=utf-8;base64,') format('woff2'), url('//at.alicdn.com/t/font_810881_vdsmyx7br89.woff?t=1565244928517') format('woff'), url('//at.alicdn.com/t/font_810881_vdsmyx7br89.ttf?t=1565244928517') format('truetype'),
    /* chrome, firefox, opera, Safari, Android, iOS 4.2+ */
  url('//at.alicdn.com/t/font_810881_vdsmyx7br89.svg?t=1565244928517#iconfont') format('svg');
  /* iOS 4.1- */
}
.iconfont {
  font-family:"iconfont" !important;
  font-size: 16px;
  font-style: normal;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}
.icon-dianzan:before { content: "\e60c"; }
.icon-fenxiang:before { content: "\e659"; }
.icon-guanbi:before { content: "\e624"; }

