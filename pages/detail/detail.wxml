                <view class="tab_tr bottom">
                    <view class="tab_td1">年份</view>
                    <view class="tab_td5">{{obj.moneyYear}}</view>
                </view>
                <view class="tab_tr bottom">
                    <view class="tab_td1">面额(元)</view>
                    <view class="tab_td5">{{obj.moneySum}}</view>
                </view>
                <view class="tab_tr bottom">
                    <view class="tab_td1">材质</view>
                    <view class="tab_td5">{{obj.moneyTexture}}</view>
                </view>

                <view class="tab_tr bottom">
                    <view class="tab_td1">质量</view>
                    <view class="tab_td5">{{obj.moneyQuality}}</view>
                </view>
                <view class="tab_tr bottom">
                    <view class="tab_td1">直径(毫米)</view>
                    <view class="tab_td5" style="font-size: {{obj.moneyDiameter.length>16?'24rpx':''}};">{{obj.moneyDiameter}}</view>
                </view>
                <view class="tab_tr bottom">
                    <view class="tab_td1">成色</view>
                    <view class="tab_td5">{{obj.moneyFineness}}</view>
                </view>
                <view class="tab_tr bottom">
                    <view class="tab_td1">重量</view>
                    <view class="tab_td5">{{obj.moneyWeight}}</view>
                </view>
                <view class="tab_tr bottom">
                    <view class="tab_td1">发行量(枚)</view>
                    <view class="tab_td5">{{obj.moneyCirculation}}</view>
                </view>
                <view class="tab_tr bottom">
                    <view class="tab_td1">系列</view>
                    <view class="tab_td5">{{obj.moneySelectItem}}</view>
                </view>
                <view class="tab_tr bottom">
                    <view class="tab_td1">铸造单位</view>
                    <view class="tab_td5">{{obj.moneyCompany}}</view>
                </view>
            </view>
            <view class="captions" wx:if="{{obj.moneyIntroduce}}"><!-- wx:if="{{options.IsShare == 1}}"-->
                <text style="color: #C6A056;">介绍: </text>{{obj.moneyIntroduce}}
            </view>
            <view class="footer">
                <view class="give-like flex" bindtap="clickGood">
                    <image src="https://www.chbice.com/imgFile/icon/{{isLike? 'praise_icon':'dislike_icon'}}.png"></image>
                </view>
                <button open-type="share" class="flex share">
                    <image src="https://www.chbice.com/imgFile/icon/share_icon.png"></image>
                </button>
                <view class="flex friends-circle" bindtap="shareFriend">
                    <image src="https://www.chbice.com/imgFile/icon/friends_icon.png"></image>
                </view>
            </view>
        </view>
    </view>

    <view wx:if="{{options.IsShare == 1}}" class="go-index" bindtap="goHome">
        <image class="go-index-img" src="../../images/pageImg/home1.png"></image>
        <view>返回主页</view>
    </view>
    <view class="showImg" catchtouchstart='' wx:if="{{isShowImg}}">
        <view wx:if="{{tempFilePath}}" class="iconfont icon-guanbi {{iphoneX?'icon-guanbi_x':''}}" bindtap="closeImg"></view>
        <view class="showImg_wrap">
            <image bindtap="lookImg" mode="widthFix" class="{{iphoneX?'showImg_img':'showImg_img1'}}" src="{{tempFilePath}}"></image>
        </view>
        <view wx:if="{{tempFilePath}}" class="showImg_save {{iphoneX?'showImg_save1':''}}" catchtap="btnClick">保存</view>
    </view>
    <view class="canvas-img">
        <canvas canvas-id="canvasShare" style="width: 100%; height: 1206rpx"></canvas>
    </view>
</view>